:@template"4!$link-blue: #44879C;


html, body {
	color: #555;
	font-family: Tahoma, Verdana, "Helvetica Neue", sans-serif;
	font-size: 11px;
	line-height: 1.5em;
}

body {
	background-color: rgb(218, 218, 218);
}

.header {
	
	.header-logo {
		display: block;
		margin-bottom: 20px;
		
		img {
			margin: 0 auto;
		}
	}
	
	#language-picker {
		float: right;
		font-size: 0.9em;
		font-weight: bold;
		margin: 0 0 5px 15px;
		width: 108px;
		
		li {
			border-left: 2px solid #000;
			float: left;
			padding: 0 5px;
			
			a {
				color: $link-blue;
				text-decoration: none;
				
				&:hover {
					text-decoration: underline;
				}
			}
			
			&:first-child {
				border: none;
			}
		}
	}

}

	
#contact-module {
	background-color: rgb(0, 97, 146);
	height: 158px;

	.contact-images {
		float: left;
		width: 326px;
	
		li {
			float: left;
			height: 70px;
			margin: 6px 0 0 6px;
		}
		
		.inactive { display: none; }
	}

	#contact-info {
		background: url('../images/contact-info-background.png') top left no-repeat;
		color: #FFF;
		float: right;
		width: 534px;
		
		.contact {
			position: relative;
			height: 158px;
						
			.contact-thumbnail-big {
				float: right;
				margin-left: 20px;
			}
			
			.contact-name {
				border-bottom: 2px solid #FFF;
				font-size: 2.0em;
				margin-left: 10px;
				padding: 10px 0;
				width: 510px - 123px - 20px;
			}
			
			.contact-content {
				margin-left: 10px;
			}
			
			.read-more {
				bottom: 10px;
				color: #FFF;
				font-weight: bold;
				left: 10px;
				position: absolute;
				text-decoration: none;
			}
		}
	}
	
	.contact-image {
	
	}
}

.container {
	margin-bottom: 20px;
	
	.top-navigation, .footer-navigation {
		float: left;
		margin: 5px 0;
		
		li {
			background: url('../images/bullet-point.gif') center left no-repeat;
			float: left;
			//font-size: 0.9em;
			margin-right: 10px;
			padding: 5px 0 5px 12px;
			
			a {
				color: $link-blue;
				font-size: 1em;
				font-weight: bold;
				text-decoration: none;
				
				&:hover {
					color: $link-blue / 1.4;
				}
			}
		}
		
		.current_page_item, .current-page-ancestor { // Class set by WordPress
			background: url('../images/bullet-point-blue.gif') center left no-repeat;
			
			a {
				color: $link-blue / 1.4;
			}
		}
	}
	
	.content {
		border-collapse: separate;
		border-spacing: 10px 0;
		margin-left: -10px; // This is a bit of a hack and should be looked into
		width: 880px;
		
		tr {
			td {
				border: 3px solid silver;
				padding: 10px;
			}
			
			.sidebar {
				width: 250px;
			}
		}	
	}
	
	.sidebar-menu {
		li {
			border-bottom: 1px solid #EEE;
			padding: 2px 0 2px 9px;
			
			a {
				color: #474845;
				font-family: Verdana;
				text-decoration: none;
				
				&:hover {
					color: #003066;
					text-decoration: underline;
				}
			}
		}
		
		.current-menu-item {
			background: url('../images/subnav-bullet-point.gif') left center no-repeat;
			margin-right: -10px;
			
			a {
				color: #003066;
				font-weight: bold;
			}
		}
	}
	
	.page {
		.title {	
			color: #474845;
			font-family: Verdana;
			font-size: 1.1em;
			font-weight: bold;
			margin-bottom: 5px;
		}
		
		.page-thumbnail {
			float: right;
			margin-left: 10px;
		}
		
		.entry {
			a {
				color: $link-blue;
				
				&:hover { color: $link-blue / 1.4; }
			}
			
			strong { font-weight: bold; }
			
			p {
				margin-bottom: 15px;
			}
			
			ul {
				list-style-image: url('../images/entry-bullet-point.gif');
				list-style-position: inside;
				margin: 0 0 15px 20px;
				padding-left: 13px;
				text-indent: -20px;
				
				li {
					margin-bottom: 10px;
				}
			}
			
			ol {
				list-style-type: decimal;
				list-style-position: inside;
				margin: 0 0 15px 20px;
				padding-left: 13px;
				text-indent: -12px;
								
				li {
					margin-bottom: 10px;
				}
			}
			
			.alignleft { 
				float: left; 
				margin: 0 10px 10px 0;
			}
		}
	}
	
	.post {
		margin-bottom: 20px;
		
		.header {
			background: rgb(234, 234, 234);
			padding: 3px 5px;
			
			.post-title {
				color: rgb(20, 29, 88);
				float: left;
				font-weight: bold;
				margin-right: 5px;
				text-decoration: none;
			}
			
			.post-date {
				color: #888;
				font-size: 0.9em;
			}
		}
		
		.page-thumbnail {
			float: right;
			margin-left: 10px;
		}
		
		.entry {
			padding: 5px;
		}
	}
	
	.single {
		.go-back {
			color: $link-blue;
			font-size: 1em;
			font-weight: bold;
			padding-left: 5px;
			text-decoration: none;

			&:hover {
				color: $link-blue / 1.4;
			}
		}
	}
	
	.single-sp_contact {
		
		table td {
			border: none;
			padding: 0 0 10px 0;
			
			ul.no-style {
				margin-bottom: 5px;
				margin-left: 0;
				padding-left: 0;
				
				li { 
					list-style-image: none; 
					margin-bottom: 5px;
					text-indent: 0;
				}
			}
			
			&.head {
				color: #003066;
				font-weight: bold;
				width: 109px;
				padding-right: 10px;
			}
			
			&.period {
				width: 119px;
			}
		}
		
		.header {
			background: none;
			margin-bottom: 15px;
			position: relative;
			
			a {
				color: $link-blue;
			}
			
			.contact-thumbnail { 
				float: left; 
				margin-bottom: 10px;
				margin-right: 20px;
			}
			
			.title { margin: 0;	}
			
			.contact-info {
				position: relative;
				top: -1px;
				
				li { margin-bottom: 1px; }
				
				table td {
					border: none;
					padding: 0;
					
					&.number {
						padding-left: 20px;
					}
				}
			}
		}
	}
	
	.contact-person {
		width: 270px;
		float: left;
		
		background: none;
		margin-top: 30px;
		position: relative;
		
		a {
			color: $link-blue;
		}
		
		.contact-thumbnail { 
			float: left; 
			margin-bottom: 10px;
			margin-right: 10px;
		}
		
		.title { margin: 0;	}
		
		.contact-info {
			position: relative;
			top: -1px;
			list-style-image: none;
			
			li { 
				margin-bottom: 1px; 
			}
			
			table td {
				border: none;
				padding: 0;
				
				&.number {
					padding-left: 10px;
				}
			}
		}	
	}
}

.newsletter {
	.wpcf7-list-item {
		display: block;
		float: left;
		width: 550px;
	}
	
	.wpcf7-list-item-label {
		float: left;
		width: 200px;
	}
	
	.text-field-label {
		float: left;
		display: block;
		padding-left: 5px;
		width: 200px;
	}
	
	input[type=text] {
		float: left;
	}
	
	input[type=submit] {
		margin-left: 207px;
	}
}

#frontpage {
	margin-top: 30px;
	float: left;
	
	.headline {
		color: $link-blue;
		font-size: 1.1em;
		font-weight: bold;
		margin-bottom: 20px;
	}
	
	.top-title {	
		color: #474845;
		font-family: Verdana;
		font-size: 1.1em;
		font-weight: bold;
		margin-bottom: 5px;
	}
	
	.sidebar-left {
		float: left;
		margin-right: 30px;
		padding: 0 13px;
		width: 200px;
		
		ul {
			margin-bottom: 20px;
			
			li {
				margin-bottom: 10px;
						
				.date {
					border-bottom: 1px solid #EEE;
					font-weight: bold;
				}
			
				.title a {
					color: #555;
					text-decoration: none;
				}
			}
		}
		
		.more-news {
			color: $link-blue;
			font-size: 1em;
			font-weight: bold;
			text-decoration: none;
		
			&:hover {
				color: $link-blue / 1.4;
			}
		}
	}
	
	.content {
		float: left;
		//margin-left: 10px;
		width: 400px;
	}
	
	.sidebar-right {
		float: right;
		text-align: right;
		width: 200px;
		
		ul li {
			margin-bottom: 0px;
			 
			a {
				color: $link-blue;
				font-size: 1.1em;
				font-weight: bold;
				text-decoration: none;
				
				&:hover {
					color: $link-blue / 1.4;
				}
			}
		}
	}
	
	.bottom-menu {
		margin: 0px 0 0 13px;
		
		.headline {
			margin-bottom: 10px;
		}
		
		.certified-adviser {
			float: right;
			
			.headline {
				text-align: right;
			}
		}
		
		.footer-navigation {
			margin: 0;
			
			li {
				line-height: 1em;
				padding: 0 0 0 15px;
				width: 110px
			}
		}
	}
}

.footer {
	background-color: rgb(0, 87, 147);
	color: #FFF;
	float: left;
	font-size: 0.9em;
	padding: 10px;
	width: 840px;
	
	.copyright {
		float: left;
	}
	
	.info {
		float: right;
		
		a { color: #FFF; }
	}
	
	#english-link li a {
		background: url('../images/english-medium.png') center center no-repeat;
		display: block;
		float: left;
		margin-left: 20px;
		width: 24px;
		
		.invisible {
			display: none;
		}
	}
}

.wp-pagenavi {
	clear: both;
	text-align: center;
	
	a, span {
		color: #555;
		margin: 2px;
		padding: 5px 5px 5px 7px;
		text-decoration: none;
	}
	
	a:hover, span.current { 
		border-color: #000; 
		color: $link-blue;
	}
	
	span.current { 
		background: #EAEAEA;
		font-weight: bold; 
	}
	
	span.pages { 
		background: none;
		border: none;
		display: block;
	}
}

.clearer {
	clear: both;
